  <object-name>rightup-leglink</object-name>
  <property>gear/gear[1]/compression-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>0</dep></entry>
    <entry><ind>1.0</ind><dep>-23</dep></entry>
  </interpolation>  
  <center>
   <x-m>-0.654</x-m>
   <y-m> 1.624</y-m>
   <z-m>-1.221</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>leftwheel</object-name>
  <property>gear/gear[0]/rollspeed-ms</property>
  <type>spin</type>
  <factor>25</factor>
  <center>
   <x-m>-0.855</x-m>
   <y-m>-1.70</y-m>
   <z-m>-1.8501</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>rightwheel</object-name>
  <property>gear/gear[1]/rollspeed-ms</property>
  <type>spin</type>
  <factor>25</factor>
  <center>
   <x-m>-0.855</x-m>
   <y-m> 1.70</y-m>
   <z-m>-1.8501</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>tailwheel</object-name>
  <property>gear/gear[2]/rollspeed-ms</property>
  <type>spin</type>
  <factor>43</factor>
  <center>
   <x-m>3.934</x-m>
   <y-m> 0.00</y-m>
   <z-m>-0.6761</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <!-- Propeller Animations -->

 <animation>
  <type>select</type>
  <object-name>propblades</object-name>
  <condition>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>700</value>
    </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>propfast</object-name>
  <condition>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
     <value>699</value>
    </greater-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>propblades</object-name>
  <object-name>propfast</object-name>
  <object-name>spinner</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>0.5</factor>
  <center>
   <x-m>-3.38</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


