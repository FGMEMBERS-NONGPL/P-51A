<!-- A wrapper/animation file for the P-51A 3D model. Original artist Paul Clausen from MSFS2004 add-ons -->

<PropertyList>

 <path>P-51A.ac</path>

<effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>propfast</object-name>
    <object-name>armorglass</object-name>
    <object-name>canopyglass</object-name>
    <object-name>aftqlass</object-name>
    <object-name>Sight</object-name>
    <object-name>LNavLightCover</object-name>
